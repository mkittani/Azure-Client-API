# AWS region to deploy resources in
MY_REGION=your_aws_region

# Name of the secret in AWS Secrets Manager containing Azure credentials
AZURE_SECRET_NAME=your_aws_secret_name

# URL of your Azure DevOps organization
AZURE_ORG_URL=https://dev.azure.com/your_org_or_username

# Azure DevOps project name
AZURE_PROJECT=your_azure_project_name

# Azure DevOps API version
AZURE_API_VERSION=your_api_version

# Amazon SNS Topic ARN for notifications
SNS_TOPIC_ARN=your_sns_topic_arn

# Discord webhook URL for sending notifications (optional)
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your_webhook_id/your_webhook_token
